<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PassgraSelec">

    <ImageButton
        android:id="@+id/btn_back2"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/ic_baseline_arrow_back_24"
        android:background="@color/white"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="30dp"></ImageButton>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <EditText
            android:id="@+id/editTextTextPersonName3"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="40dp"
            android:ems="10"
            android:gravity="center"
            android:inputType="textPersonName"
            android:text="선택 졸업 인증제 상세 자료" />

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">


                <LinearLayout

                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="30dp"
                    android:layout_marginRight="30dp"
                    android:layout_marginBottom="20dp"
                    android:background="#E0E9EF"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/view_pass"
                        android:layout_width="315dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="30dp"
                        android:background="#E0E9EF"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="150dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="8dp"
                            android:background="@color/white"
                            android:forceDarkAllowed="false"
                            android:gravity="center"
                            android:text="사회봉사(택1)"
                            android:textSize="15sp"
                            android:textStyle="bold" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="103dp"
                            android:layout_marginTop="4dp"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="5dp"
                                    android:forceDarkAllowed="false"
                                    android:text="사회봉사 30시간이상"
                                    android:textSize="13dp"></TextView>


                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="5dp"
                                    android:forceDarkAllowed="false"
                                    android:text="국내,외 봉사프로그램 5일 이상 참가"

                                    android:textSize="13dp"></TextView>


                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="5dp"
                                    android:forceDarkAllowed="false"
                                    android:text="1학기 당 1회 헌혈하여 총 3회 이상"
                                    android:textSize="13dp"></TextView>


                                <TextView
                                    android:id="@+id/textView39"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="5dp"
                                    android:forceDarkAllowed="false"
                                    android:text="총장이 인정하는 1학기 이상의 교내 봉사활동"
                                    android:textSize="13dp"></TextView>

                            </LinearLayout>


                        </LinearLayout>


                    </LinearLayout>


                </LinearLayout>

                <LinearLayout

                    android:layout_width="match_parent"
                    android:layout_height="160dp"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="30dp"
                    android:layout_marginBottom="20dp"
                    android:background="#E0E9EF"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="170dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="8dp"
                        android:background="@color/white"
                        android:forceDarkAllowed="false"
                        android:gravity="center"
                        android:text="글로벌 리더십(택1)"
                        android:textSize="15sp"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="10dp"
                        android:forceDarkAllowed="false"
                        android:text="해외현장체험 참가 7일 이상"
                        android:textSize="13dp"></TextView>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="5dp"
                        android:forceDarkAllowed="false"
                        android:text="Global Pioneer Program 참가 7일 이상"
                        android:textSize="13dp"></TextView>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="5dp"
                        android:forceDarkAllowed="false"
                        android:text="해외 단기 교환 프로그램 참가 7일 이상"
                        android:textSize="13dp"></TextView>


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="5dp"
                        android:forceDarkAllowed="false"
                        android:text="해외 인턴십 수료 4주 이상"
                        android:textSize="13dp"></TextView>


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="5dp"
                        android:forceDarkAllowed="false"
                        android:text="해외 교환학생 수료 1학기 이상"
                        android:textSize="13dp"

                        ></TextView>


                </LinearLayout>

                <LinearLayout

                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="30dp"
                    android:layout_marginBottom="20dp"
                    android:background="#E0E9EF"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="170dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="8dp"
                        android:background="@color/white"
                        android:forceDarkAllowed="false"
                        android:gravity="center"
                        android:text="독서"
                        android:textSize="15sp"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="314dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="10dp"
                        android:forceDarkAllowed="false"
                        android:text="위원회에서 선정한 도서를 읽고, 컴퓨터 평가 시스템을        통한 문제은행 평가에 응시하여 70점 이상"
                        android:textSize="13dp"></TextView>
                    <TextView
                        android:layout_width="281dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="5dp"
                        android:forceDarkAllowed="false"
                        android:text="취득한 도서가 7권 이상인 경우"
                        android:textSize="13dp"></TextView>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="30dp"
                    android:layout_marginBottom="20dp"
                    android:background="#E0E9EF"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="170dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="8dp"
                        android:background="@color/white"
                        android:forceDarkAllowed="false"
                        android:gravity="center"
                        android:text="GNU 인성(택1)"
                        android:textSize="15sp"
                        android:textStyle="bold" />


                    <TextView
                        android:layout_width="290dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="10dp"
                        android:forceDarkAllowed="false"
                        android:text="교과목 이수"
                        android:textSize="13dp"></TextView>

                    <TextView
                        android:layout_width="329dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="5dp"
                        android:forceDarkAllowed="false"
                        android:text="기초교육원 인성개발 프로그램 12시간 이수"
                        android:textSize="13dp"></TextView>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="130dp"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="30dp"
                    android:layout_marginBottom="30dp"
                    android:background="#E0E9EF"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="230dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="8dp"
                        android:background="@color/white"
                        android:forceDarkAllowed="false"
                        android:gravity="center"
                        android:text="진로탐색(아래기준 모두이수)"
                        android:textSize="15sp"
                        android:textStyle="bold" />


                    <TextView
                        android:layout_width="290dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="10dp"
                        android:forceDarkAllowed="false"
                        android:text="진로,취업상담 1회이상이수"
                        android:textSize="13dp"></TextView>

                    <TextView
                        android:layout_width="290dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="5dp"
                        android:forceDarkAllowed="false"
                        android:text="비교과 프로그램 3회이상 이수"
                        android:textSize="13dp"></TextView>

                    <TextView
                        android:layout_width="290dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="5dp"
                        android:forceDarkAllowed="false"
                        android:text="설명회 및 박람회 2회이상 이수"
                        android:textSize="13dp"></TextView>

                </LinearLayout>

            </LinearLayout>
        </ScrollView>


    </LinearLayout>


</FrameLayout>

